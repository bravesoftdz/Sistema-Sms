  object upListaSmsEmail: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM Enviados'
      'WHERE'
      '  Enviados.id = :OLD_id')
    InsertSQL.Strings = (
      'INSERT INTO Enviados'
      
        '  (Cnpj, celular, Enviado, Mensagem, Remetente, Tipo, CodigoText' +
        'oEmail, '
      '   data, hora, id)'
      'VALUES'
      
        '  (:Cnpj, :celular, :Enviado, :Mensagem, :Remetente, :Tipo, :Cod' +
        'igoTextoEmail, '
      '   :data, :hora, :id)')
    ModifySQL.Strings = (
      'UPDATE Enviados SET'
