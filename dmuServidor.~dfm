  object upSmsRecebidos: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM `Recebidas`'
      'WHERE'
      '  `Recebidas`.id = :OLD_id')
    InsertSQL.Strings = (
      'INSERT INTO `Recebidas`'
      '  (id, Numero, Mensagem, Data, Hora, Original)'
      'VALUES'
      '  (:id, :Numero, :Mensagem, :Data, :Hora, :Original)')
    ModifySQL.Strings = (
      'UPDATE `Recebidas` SET'
      '  id = :id,'
      '  Numero = :Numero,'
      '  Mensagem = :Mensagem,'
      '  Data = :Data,'
      '  Hora = :Hora,'
      '  Original = :Original'
      'WHERE'
      '  `Recebidas`.id = :OLD_id')
    UseSequenceFieldForRefreshSQL = False
    Left = 290
