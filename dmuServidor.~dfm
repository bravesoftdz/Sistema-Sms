      '  Remetente = :Remetente,'
      '  Tipo = :Tipo,'
      '  CodigoTextoEmail = :CodigoTextoEmail,'
      '  data = :data,'
      '  hora = :hora,'
      '  id = :id'
      'WHERE'
      '  Enviados.id = :OLD_id')
